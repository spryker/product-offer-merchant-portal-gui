{
  "name": "spryker/product-offer-merchant-portal-gui",
  "type": "library",
  "description": "ProductOfferMerchantPortalGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/currency": "^3.1.0",
    "spryker/gui-table": "^0.1.0",
    "spryker/kernel": "^3.51.0",
    "spryker/locale": "^3.0.0",
    "spryker/merchant-product-offer": "^0.5.0",
    "spryker/merchant-stock": "^0.3.0",
    "spryker/merchant-user": "^0.5.0",
    "spryker/price-product": "^2.0.0 || ^4.0.0",
    "spryker/product": "^5.0.0 || ^6.0.0",
    "spryker/product-image": "^3.3.0",
    "spryker/product-offer": "^0.4.0",
    "spryker/product-offer-stock": "^0.4.0",
    "spryker/product-offer-validity": "^0.2.0",
    "spryker/product-validity": "^1.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/store": "^1.1.0",
    "spryker/symfony": "^3.0.0",
    "spryker/transfer": "^3.18.0",
    "spryker/translator": "^1.1.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zed-ui": "^0.3.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
